{
  "name": "fotobox-frontend",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "mvse",
  "sourceRoot": "packages/fotobox/frontend/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/fotobox/frontend",
        "index": "packages/fotobox/frontend/src/index.html",
        "main": "packages/fotobox/frontend/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "packages/fotobox/frontend/tsconfig.app.json",
        "assets": [
          "packages/fotobox/frontend/src/favicon.ico",
          "packages/fotobox/frontend/src/assets"
        ],
        "styles": [
          "node_modules/font-awesome/css/font-awesome.css",
          "node_modules/bootstrap/dist/css/bootstrap.min.css",
          "packages/fotobox/frontend/src/styles.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "fotobox-frontend:build:production"
        },
        "development": {
          "buildTarget": "fotobox-frontend:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "fotobox-frontend:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/fotobox/frontend/jest.config.ts"
      }
    }
  }
}
